{
  "homepage": "https://edujura.github.io/my-react-app-with-ts/",
  "name": "my-react-app-with-ts",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "ts-jest": "^29.1.2"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^10.4.19",
    "bootswatch": "^4.5.3",
    "postcss": "^8.4.38",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "selenium-webdriver": "^4.19.0",
    "tailwindcss": "^3.4.3",
    "typed.js": "^2.1.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "pre-ci": "rm -rf node_modules",
    "ci": "yarn install --immutable --immutable-cache --check-cache",
    "test": "react-scripts test",
    "test:e2e": "react-scripts test --testMatch=**\\.test\\.e2e\\.ts",
    "local:test:e2e": "REACT_APP_SELENIUM_TEST_URL=http://localhost:3000/my-react-app-with-ts/ yarn test:e2e",
    "prod:test:e2e": "yarn test:e2e"
  },
  "jest": {
    "moduleNameMapper": {
      "node:events": "<rootDir>/node_modules/events/"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
